def laberinto(dimension,muros):
    laberinto = []
    for i in range(dimension):
        fila = []
        for i in range(dimension):
            if tuple([i,j]) in muros:
                fila.append('X')
            else:
                fila.append(' ')
        laberinto.append(fila)
    return laberinto
for i in laberinto:
    print(''.join(i))